{
  "name": "dashboard-editor",
  "version": "1.0.0",
  "main": "dist/main/index.cjs",
  "author": "Joel <xiao2731962667@163.com>",
  "license": "MIT",
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "build": "vue-tsc --project src/renderer/tsconfig.json --noEmit && node scripts/build.mjs && electron-builder",
    "tsc": "vue-tsc --project src/renderer/tsconfig.json --noEmit",
    "es:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.vue,.html",
    "es:lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.html",
    "style:fix": "stylelint . --fix .css,.less,.scss",
    "style:lint": "stylelint . --aei .css,.less,.scss",
    "format": "prettier --write ./**/*.{vue,ts,js,scss,json,md}",
    "test": "jest",
    "log": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0  -n ./changelog-option.js",
    "prepare": "husky install"
  },
  "changelog": {
    "bugsUrl": "https://github.com/huoyou/vue_cli3/issues/",
    "emojis": true,
    "authorName": true,
    "authorEmail": true
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,vue,html}": ["npm run es:fix"],
    "./**/*.{css,less,scss}": ["npm run style:fix"],
    "./**/*.{vue,ts,js,scss,json,md}": ["npm run format"]
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "env": {
    "//": "Used in build scripts",
    "HOST": "127.0.0.1",
    "PORT": 3344
  },
  "keywords": ["vite", "electron", "vue3", "rollup"],
  "dependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@types/autoprefixer": "^10.2.0",
    "@types/axios": "^0.14.0",
    "@types/eslint": "^8.37.0",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/prettier": "^3.0.0",
    "@types/pug": "^2.0.6",
    "@types/sass": "^1.45.0",
    "@types/stylelint": "^14.0.0",
    "@types/vue-router": "^2.0.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/vue3-jest": "^29.2.3",
    "amfe-flexible": "^2.2.1",
    "autoprefixer": "^10.4.14",
    "axios": "^1.3.6",
    "conventional-changelog-cli": "^4.1.0",
    "conventional-changelog-custom-config": "^0.3.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.11.0",
    "husky": "^9.0.10",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^15.2.1",
    "naive-ui": "^2.34.3",
    "pinia": "^2.0.35",
    "pinia-plugin-persist": "^1.0.0",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^3.2.5",
    "pug": "^3.0.2",
    "register-service-worker": "^1.7.2",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.62.1",
    "stylelint": "^15.6.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "vfonts": "^0.0.3",
    "vite-plugin-compression": "^0.5.1",
    "vue": "^3.2.47",
    "vue-global-api": "^0.4.1",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@tauri-apps/cli": "^1.2.3",
    "@testing-library/jest-dom": "^6.4.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/test-utils": "^2.3.2",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "vite": "^5.0.12",
    "vite-plugin-vue-devtools": "^7.0.14",
    "vue-tsc": "^1.4.4",
    "yarn-upgrade-all": "^0.7.2",
    "vite-plugin-resolve": "^1.4.3",
    "electron": "16.0.8",
    "electron-builder": "^22.14.5"
  }
}
